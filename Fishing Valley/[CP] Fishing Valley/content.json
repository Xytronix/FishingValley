{
  "Format": "2.0.0",
  "ConfigSchema": {
    "EditFarm": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds teleportation tiles on the farm to Fishing Valley."
    },
    "AddFishesToTown": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds fishes to Town."
    },
    "EasyFishing": {
      "AllowValues": "true, false",
      "Default": "false",
      "Description": "Make your life on the sea easier by making fishing less difficult."
    },
    "EnableFishes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Adds fishes to Fishing Valley."
    },
    "EnableInsects": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Adds bugs to Fishing Valley."
    },
    "EnableGiftTastes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds gift tastes to NPCs."
    },
    "EnableCookingRecipes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds cooking recipes. SpaceCore required."
    },
    "EnableCraftingRecipes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds crafting recipes. SpaceCore required."
    },
    "EnableMusic": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Adds music to Fishing Valley."
    },
    "EnableMail": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Adds mail to Fishing Valley."
    },
    "EnableJukeboxAmbient": {
      "AllowValues": "true, false",
      "Default": "false",
      "Description": "Adds Ambient Tracks to the Jukebox."
    },
    "CategoryOverride": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Override fish categories to the correct types. SpaceCore required."
    },
    "EnableRailroad": {
      "AllowValues": "true, false",
      "Default": "false",
      "Description": "Adds vanilla Railroad location to the Minecart network."
    },
    "EnableMinecartNetwork": {
      "AllowValues": "true, false",
      "Default": "false",
      "Description": "Adds the minecart as a new network"
    }
  },

  "Changes": [
    {
      "LogName": "Loading Tilesheets.",
      "Action": "Load",
      "Target": "Maps/bluelava, Maps/fallcliff, Maps/fishingfestival, Maps/flashExtras, Maps/Floors_Set_1, Maps/Floors_Set_2, Maps/greylava, Maps/JojaEnergyInterior, Maps/jukebox, Maps/Mantleby, Maps/mine_dangerous, Maps/mine_dark_dangerous, Maps/mine_desert_dangerous, Maps/pizza-place, Maps/purplelava, Maps/redlava, Maps/redlavafloor, Maps/spring_GrampletonSuburbsTilesheet, Maps/summer_extra3, Maps/summer_IslandTiles, Maps/waterfall_tileset, Maps/zcustom_fish_shop, Maps/zFshing_people_front, Maps/Zmushroom, Maps/zpeople, Maps/zswampplant, Maps/Ztwater_tree",
      "FromFile": "assets/Tilesheets/{{targetWithoutPath}}.png"
    },

