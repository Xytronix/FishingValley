{
  "Format": "2.0",
  "ConfigSchema": {
    "EditFarm": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds teleportation tiles on the farm to Fishing Valley."
    },
    "AddFishesToTown": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds fishes to Town."
    },
    "EasyFishing": {
      "AllowValues": "true, false",
      "Default": "false",
      "Description": "Make your life on the sea easier by making fishing less difficult."
    },
    "EnableFishes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Adds fishes to Fishing Valley."
    },
    "EnableInsects": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Adds bugs to Fishing Valley."
    },
    "EnableGiftTastes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds gift tastes to NPCs."
    },
    "EnableCookingRecipes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds cooking recipes. SpaceCore required."
    },
    "EnableCraftingRecipes": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "Adds crafting recipes. SpaceCore required."
    },
    "EnableMusic": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Adds music to Fishing Valley."
    },
    "EnableJukeboxAmbient": {
      "AllowValues": "true, false",
      "Default": "false",
      "Description": "Adds Ambient Tracks to the Jukebox."
    },
    "CategoryOverride": {
      "AllowValues": "true, false",
      "Default": "true",
      "Description": "(Recommended) Override fish categories to the correct types. SpaceCore required."
    }
  },

  "CustomLocations": [
    {
      "Name": "FishingValley.FishingResortExterior",
      "FromMapFile": "assets/Maps/FishingResortExterior.tmx"
    },

    {
      "Name": "FishingValley.FishingResortInterior",
      "FromMapFile": "assets/Maps/FishingResortInterior.tmx"
    },

    {
      "Name": "FishingValley.ForestSwamp",
      "FromMapFile": "assets/Maps/ForestSwamp.tmx"
    },

    {
      "Name": "FishingValley.FungusHouse",
      "FromMapFile": "assets/Maps/FungusHouse.tmx"
    },

    {
      "Name": "FishingValley.JungleBay",
      "FromMapFile": "assets/Maps/JungleBay.tmx"
    },

    {
      "Name": "FishingValley.JungleCave",
      "FromMapFile": "assets/Maps/JungleCave.tmx"
    },

    {
      "Name": "FishingValley.JungleFalls",
      "FromMapFile": "assets/Maps/JungleFalls.tmx"
    },

    {
      "Name": "FishingValley.JungleJapesMain",
      "FromMapFile": "assets/Maps/JungleJapesMain.tmx"
    },

    {
      "Name": "FishingValley.JungleRiver",
      "FromMapFile": "assets/Maps/JungleRiver.tmx"
    },

    {
      "Name": "FishingValley.Serenityriver",
      "FromMapFile": "assets/Maps/JungleJapesMain.tmx"
    },

    {
      "Name": "FishingValley.WaterfallCave",
      "FromMapFile": "assets/Maps/WaterfallCave.tmx"
    },

    {
      "Name": "FishingValley.HoshiroTaniMajula",
      "FromMapFile": "assets/Maps/HTMP01.tmx"
    },

    {
      "Name": "FishingValley.OCaminhodasChamasGemeas",
      "FromMapFile": "assets/Maps/HTMP02.tmx"
    },

    {
      "Name": "FishingValley.LaMilladelJardiinRea",
      "FromMapFile": "assets/Maps/HTMP03.tmx"
    }
  ],

  "Changes": [
    {
      "LogName": "Loading Music.",
      "Action": "Include",
      "When": { "EnableMusic": true },
      "FromFile": "code/Music/Music.json, code/Music/Jukebox.json"
    },

    {
      "LogName": "Loading Locations.",
      "Action": "Include",
      "FromFile": "code/Locations/LocationsData.json, code/Locations/MapEdits.json, code/Locations/Strings.json, code/Locations/Warps.json"
    },

    {
      "LogName": "Loading Fishes.",
      "Action": "Include",
      "When": { "EnableFishes": true },
      "FromFile": "code/Fishes/FishData.json, code/Fishes/Aquarium.json, code/Fishes/FishPond.json"
    },

    {
      "LogName": "Loading Insects.",
      "Action": "Include",
      "When": { "EnableInsects": true },
      "FromFile": "code/Insects/BugData.json"
    },

    {
      "LogName": "Loading NPCs.",
      "Action": "Include",
      "When": { "EnableGiftTastes": true },
      "FromFile": "code/NPCs/GiftTastes.json"
    },

    {
      "LogName": "Loading Shops.",
      "Action": "Include",
      "FromFile": "code/Shops/Shops.json"
    },

    {
      "Action": "Load",
      "Target": "Data/Objects/spring_fsnovel",
      "FromFile": "assets/Objects/spring_fsnovel.png"
    },

    {
      "Action": "Load",
      "Target": "Data/Objects/summer_fsnovel",
      "FromFile": "assets/Objects/summer_fsnovel.png"
    },

    {
      "Action": "Load",
      "Target": "Data/Objects/fall_fsnovel",
      "FromFile": "assets/Objects/fall_fsnovel.png"
    },

    {
      "Action": "Load",
      "Target": "Data/Objects/winter_fsnovel",
      "FromFile": "assets/Objects/winter_fsnovel.png"
    }
  ]
}
