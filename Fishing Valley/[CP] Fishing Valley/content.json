    {
      "LogName": "Loading Music.",
      "Action": "Include",
      "When": { "EnableMusic": true },
      "FromFile": "code/Music/Music.json, code/Music/Jukebox.json"
    },

    {
      "LogName": "Loading Locations.",
      "Action": "Include",
      "FromFile": "code/Locations/LocationContext.json, code/Locations/LocationsData.json, code/Locations/MapEdits.json, code/Locations/Strings.json, code/Locations/Warps.json"
    },

